package hnch.referencia.core.bean.asistencial.citas;

import java.util.Date;

import hnch.referencia.base.bean.BaseBean;
import hnch.referencia.core.bean.general.PersonaBean;
import hnch.referencia.core.bean.general.TablaBean;

public class ProgramacionMedicoCitaBean extends BaseBean {

	private String codigoProgramacionDH;
	private String nroPeriodoDH;
	
	private String codigoTiempoEspecialidad;
	private Date fechaProgramacion;
	private String horaInicio;
	private String horaTermino;
	private TablaBean ambiente;
	private PersonaBean medico;
	private TablaBean actividad;
	private TablaBean especialidad;

	private int cantidadNuevos;
	private String tiempoAtencionNuevo;
	private String tiempoAtencionContinuador;
	private int porcetaje;
	private int cantidadProgramados;
	private int cantidadDisponible;
	private String strFechaProgramacion;
	private TablaBean servicio; 
	public ProgramacionMedicoCitaBean() {
		super();
	}

	
	public String getCodigoProgramacionDH() {
		return codigoProgramacionDH;
	}
	public void setCodigoProgramacionDH(String codigoProgramacionDH) {
		this.codigoProgramacionDH = codigoProgramacionDH;
	}
	public String getCodigoTiempoEspecialidad() {
		return codigoTiempoEspecialidad;
	}
	public void setCodigoTiempoEspecialidad(String codigoTiempoEspecialidad) {
		this.codigoTiempoEspecialidad = codigoTiempoEspecialidad;
	}

	public Date getFechaProgramacion() {
		return fechaProgramacion;
	}
	public void setFechaProgramacion(Date fechaProgramacion) {
		this.fechaProgramacion = fechaProgramacion;
	}
	public String getHoraInicio() {
		return horaInicio;
	}
	public void setHoraInicio(String horaInicio) {
		this.horaInicio = horaInicio;
	}
	public String getHoraTermino() {
		return horaTermino;
	}
	public void setHoraTermino(String horaTermino) {
		this.horaTermino = horaTermino;
	}
	public TablaBean getAmbiente() {
		return ambiente;
	}
	public void setAmbiente(TablaBean ambiente) {
		this.ambiente = ambiente;
	}
	
	public String getNroPeriodoDH() {
		return nroPeriodoDH;
	}


	public void setNroPeriodoDH(String nroPeriodoDH) {
		this.nroPeriodoDH = nroPeriodoDH;
	}


	public PersonaBean getMedico() {
		if (medico == null) {
			medico = new PersonaBean();
		}
		return medico;
	}
	public void setMedico(PersonaBean medico) {
		this.medico = medico;
	}
	public TablaBean getActividad() {
		if (actividad == null) {
			actividad = new TablaBean();
		}
		return actividad;
	}
	public void setActividad(TablaBean actividad) {
		this.actividad = actividad;
	}
	public TablaBean getEspecialidad() {
		if (especialidad == null) {
			especialidad = new TablaBean();
		}
		return especialidad;
	}
	public void setEspecialidad(TablaBean especialidad) {
		this.especialidad = especialidad;
	}
	public int getCantidadNuevos() {
		return cantidadNuevos;
	}
	public void setCantidadNuevos(int cantidadNuevos) {
		this.cantidadNuevos = cantidadNuevos;
	}
	public String getTiempoAtencionNuevo() {
		return tiempoAtencionNuevo;
	}
	public void setTiempoAtencionNuevo(String tiempoAtencionNuevo) {
		this.tiempoAtencionNuevo = tiempoAtencionNuevo;
	}
	public String getTiempoAtencionContinuador() {
		return tiempoAtencionContinuador;
	}
	public void setTiempoAtencionContinuador(String tiempoAtencionContinuador) {
		this.tiempoAtencionContinuador = tiempoAtencionContinuador;
	}
	public int getPorcetaje() {
		return porcetaje;
	}
	public void setPorcetaje(int porcetaje) {
		this.porcetaje = porcetaje;
	}
	public int getCantidadProgramados() {
		return cantidadProgramados;
	}
	public void setCantidadProgramados(int cantidadProgramados) {
		this.cantidadProgramados = cantidadProgramados;
	}
	public int getCantidadDisponible() {
		return cantidadDisponible;
	}
	public void setCantidadDisponible(int cantidadDisponible) {
		this.cantidadDisponible = cantidadDisponible;
	} 

	public String getStrFechaProgramacion() {
		return strFechaProgramacion;
	} 

	public void setStrFechaProgramacion(String strFechaProgramacion) {
		this.strFechaProgramacion = strFechaProgramacion;
	} 
	public TablaBean getServicio() {
		if (servicio == null) {
			servicio = new TablaBean();
		}
		return servicio;
	}

	public void setServicio(TablaBean servicio) {
		this.servicio = servicio;
	}

}
