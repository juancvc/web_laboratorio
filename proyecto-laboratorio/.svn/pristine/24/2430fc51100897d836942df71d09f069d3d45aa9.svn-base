	public AntecedentesCirugiaBean(){
		super();
	}



	public PersonalBean getCirujano() {
		return cirujano;
	}

	public void setCirujano(PersonalBean cirujano) {
		this.cirujano = cirujano;
	}

	public Date getFechOperacion() {
		return fechOperacion;
	}

	public void setFechOperacion(Date fechOperacion) {
		this.fechOperacion = fechOperacion;
	}



	public TablaBean getProcCirugiaTarifario() {
		return procCirugiaTarifario;
	}



	public void setProcCirugiaTarifario(TablaBean procCirugiaTarifario) {
		this.procCirugiaTarifario = procCirugiaTarifario;
	}



	public PacienteBean getPaciente() {
		return paciente;
	}



	public void setPaciente(PacienteBean paciente) {
		this.paciente = paciente;
	}



	public String getCirugiaManual() {
		return cirugiaManual;
	}



	public void setCirugiaManual(String cirugiaManual) {
		this.cirugiaManual = cirugiaManual;
	}


