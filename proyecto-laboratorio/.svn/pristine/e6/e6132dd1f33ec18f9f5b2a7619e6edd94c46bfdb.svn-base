package hnch.referencia.core.bean.general.antecedentes;

import hnch.referencia.base.bean.BaseBean;
import hnch.referencia.core.bean.general.PacienteBean;
import hnch.referencia.core.bean.general.TablaBean;




public class AntecedentesEnfermedadBean extends BaseBean{
	private String codCIEX;
	private String nombEnfermedad;
	private PacienteBean paciente;
	private TablaBean enfermedad;  //enfermedad con codigo CIEX
	private String enfermedadManual;   //Enfermedad en caso de desconocer el CIEX
	
	public  AntecedentesEnfermedadBean() {
		super();
	}

	public String getCodCIEX() {
		return codCIEX;
	}

	public void setCodCIEX(String codCIEX) {
		this.codCIEX = codCIEX;
	}

	public String getNombEnfermedad() {
		return nombEnfermedad;
	}

	public void setNombEnfermedad(String nombEnfermedad) {
		this.nombEnfermedad = nombEnfermedad;
	}

	public PacienteBean getPaciente() {
		return paciente;
	}

	public void setPaciente(PacienteBean paciente) {
		this.paciente = paciente;
	}

	public TablaBean getEnfermedad() {
		return enfermedad;
	}

	public void setEnfermedad(TablaBean enfermedad) {
		this.enfermedad = enfermedad;
	}

	public String getEnfermedadManual() {
		return enfermedadManual;
	}

	public void setEnfermedadManual(String enfermedadManual) {
		this.enfermedadManual = enfermedadManual;
	}
	
	
}
