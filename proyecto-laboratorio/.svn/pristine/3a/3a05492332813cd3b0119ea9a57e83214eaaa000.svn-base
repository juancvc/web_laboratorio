  width: 100%;
  background: #fff;
  border-left:1px solid #E4E7ED;
  border-right:1px solid #E4E7ED;
  #main-imagen-ci{
    display: block;
    float: left;
    width: calc(100% - 260px);
    background: #fff;
    position:relative;
    .media-toolbar {
        right: 300px;
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        z-index: 100;
        padding: 15px;
        border: 0 solid #ddd;
        overflow: hidden;
        background: #fff;
        .form-group{
          margin-bottom: 0;
        }
    }
.h-title-hidden{
  font-size: 20px;
  line-height: 28px;
  font-weight: 400;
  margin: 0
}
  }
  #main-detalle-ci{
    display: block;
    width: 260px;
    background:#f3f3f3;
    float: right;
    bottom: 0;
    padding: 0 16px 24px;
    z-index: 75;
    border-left: 1px solid #ddd;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    height: 400px;
    .filename {
        font-weight: 600;
        color: #444;
        word-wrap: break-word;
    }
    .details {
        float: left;
        font-size: 12px;
        max-width: 100%;
