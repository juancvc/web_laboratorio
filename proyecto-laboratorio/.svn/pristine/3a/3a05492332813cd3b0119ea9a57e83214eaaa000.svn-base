    }
  }
  .title-dialog-detalle{
    position: relative;
    font-weight: 600;
    text-transform: uppercase;
    font-size: 12px;
    color: #666;
    margin: 24px 0 8px;
  }
}
.delete-attachment{
  display: inline;
  padding: 0;
  color: #bc0b0b;
  &:hover{
    color:red;
  }
}
.button.button-hero {
    border-color: #ccc;
    background: #f7f7f7;
    -webkit-box-shadow: 0 1px 0 #ccc;
    box-shadow: 0 1px 0 #ccc;
    vertical-align: top;
    font-size: 14px;
    height: 46px;
    line-height: 44px;
    padding: 0 36px;
    display: table;
}
.button-link {
    margin: 0;
    padding: 0;
    -webkit-box-shadow: none;
    box-shadow: none;
    border: 0;
    -webkit-border-radius: 0;
    border-radius: 0;
    background: 0 0;
    outline: 0;
    cursor: pointer;
}
.attachment-info .thumbnail {
    position: relative;
    float: left;
    max-width: 120px;
    max-height: 120px;
    margin-top: 5px;
    margin-right: 10px;
    margin-bottom: 5px;
    img{
      display: table;
      width: 100%;
    }
}
.attachment-info .thumbnail-audio{
    width: 100%;
    max-height: inherit;
    max-width: inherit;
  audio, video{
        display: table;
        width: 100%;
      }
}



.attachments-browser .attachments {
    padding: 2px 8px 8px;
}
.attachments-browser .attachments{
    position: absolute;
    top: 60px;
    left: 0;
    height: 340px;
    bottom: 0;
    overflow: auto;
    outline: 0;
    width: 100%;
    margin-bottom: 0;
}
.wp-core-ui{
   .attachment-preview {
      position: relative;
      -webkit-box-shadow: inset 0 0 15px rgba(0,0,0,.1), inset 0 0 0 1px rgba(0,0,0,.05);
      box-shadow: inset 0 0 15px rgba(0,0,0,.1), inset 0 0 0 1px rgba(0,0,0,.05);
      background: #eee;
      cursor: pointer;
  }
  .attachment {
    position: relative;
    float: left;
    padding: 8px;
    margin: 0;
    color: #444;
    cursor: pointer;
    list-style: none;
    text-align: center;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    width: 25%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    .landscape img, .landscape video, .landscape audio {
        max-height: 100%;
    }
    .thumbnail {
        overflow: hidden;
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        opacity: 1;
        -webkit-transition: opacity .1s;
        transition: opacity .1s;
        margin-bottom: 0;
        &:after{
          content: "";
          display: block;
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
          -webkit-box-shadow: inset 0 0 0 1px rgba(0,0,0,.1);
          box-shadow: inset 0 0 0 1px rgba(0,0,0,.1);
          overflow: hidden;
        }
        .centered {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            -webkit-transform: translate(50%,50%);
            -ms-transform: translate(50%,50%);
            transform: translate(50%,50%);
            img, video, audio {
                position: absolute;
                top: 0;
                left: 0;
                max-height: 100%;
                -webkit-transform: translate(-50%,-50%);
                -ms-transform: translate(-50%,-50%);
                transform: translate(-50%,-50%);
            }
        }
    }

    .media-box {
        &:after{
          position: relative;
        }
    }
  }

  .select-item{
      -webkit-box-shadow: inset 0 0 0 3px #fff, inset 0 0 0 7px red;
      box-shadow: inset 0 0 0 3px #fff, inset 0 0 0 7px red;
  }

   .attachment-preview {
      position: relative;
      -webkit-box-shadow: inset 0 0 15px rgba(0,0,0,.1), inset 0 0 0 1px rgba(0,0,0,.05);
      box-shadow: inset 0 0 15px rgba(0,0,0,.1), inset 0 0 0 1px rgba(0,0,0,.05);
      background: #eee;
      cursor: pointer;
      &::before {
          content: "";
          display: block;
          padding-top: 100%;
      }
  }
}

