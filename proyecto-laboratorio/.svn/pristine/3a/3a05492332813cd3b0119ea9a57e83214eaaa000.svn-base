// Definiendo modal para Material
#content-md-materail{
  display: table;
  width: 100%;
}
#left-md-material{
  display: block;
  position:relative;
  float: left;
  width: 300px;
}
#md-material-search{
  position:absolute;
  top: 0;
  width: 100%;
  background: #fff;
  box-shadow: 0 3px 4px rgba(0,0,0,0.3);
  z-index: 1;
}
#list-md-material{
  margin-top: 90px;
  .scroll-example {
      padding: 0;
      border: 0;
  }
  .media{
    background: #fff;
    margin-bottom: 5px;
    padding: 10px!important;
    cursor:pointer;
  }
  .media-active{
    background: #fbc02d;
    color: #333;
  }
  .media-body{
    .media-heading{
      line-height: 15px;
      color:#000;
    }
