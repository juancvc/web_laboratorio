}
// css para notificaciones
.media-list-linked{
  .media {
      padding: 0;
      margin-top: 0px;
      background: #F5F5F5;
  }
  .border-left-info{
    background: #fff;
    .media-heading{
      font-weight: 600;
      color:#000;
    }
  }
  .media-link {
      display: block;
      padding-top: 8px;
      padding-right: 12px;
      padding-bottom: 10px;
      padding-left: 12px;
      color: #222;
  }
  .media-body {
      font-size: 11px;
      line-height: 1.8;
      color: #222;
      text-align: left;
      line-height: 1.6;
      overflow: visible;
      position:relative;
  }
  .media-heading {
      margin-top: 0px;
      margin-bottom: 2px;
      font-size: 14px;
      line-height: 1.8;
      color: #555;
      font-weight: 600;
      text-align: left;
      display: block;
  }
  .media-options {
      position: absolute;
      top: 3px;
      right: 20px;
      // display: none;
      .star {
          float: left;
         label {
            display: inline-block;
            cursor: pointer;
            position: relative;
            font-size: 16px;
            -webkit-transition: border 0.2s linear 0s,color 0.2s linear 0s;
            -moz-transition: border 0.2s linear 0s,color 0.2s linear 0s;
            transition: border 0.2s linear 0s,color 0.2s linear 0s;
            margin-bottom: 13px;
        }
      }
      >span.date {
          float: left;
          padding-right: 10px;
      }
  }
  .position-left {
      margin-right: 6px;
  }

  .label-danger {
      border-color: #F44336;
  }
  .notification-active{
    background: #fbc02d;
  }
}
