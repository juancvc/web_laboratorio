package hnch.referencia.core.bean.general;

import java.sql.Date;
import java.util.Arrays;

import javax.persistence.Column;
import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;


@XmlRootElement(name="pacienteReniec",namespace="pe.edu.galaxy.training.ws.rest.pacienteReniec")
public class PacienteReniecBean extends PersonaBean{
	
	
	private String codigoRespuesta;
	private String nroDni;
	private String codigoVerificacion;
	private String apellidoPaterno;
	private String apellidoMaterno;
	private String nombres;
	private String condicionCasado;	
	private String fechaNacimiento;
	private String nombrePadre;
	private String nombreMadre;
	private byte[] fotoPaciente;
	
	
	
	public PacienteReniecBean() {
		super();
	}


	@XmlElement
	public String getCodigoRespuesta() {
		return codigoRespuesta;
	}



	public void setCodigoRespuesta(String codigoRespuesta) {
		this.codigoRespuesta = codigoRespuesta;
	}


	@XmlElement
	public String getNroDni() {
		return nroDni;
	}



	public void setNroDni(String nroDni) {
		this.nroDni = nroDni;
	}


	@XmlElement
	public String getCodigoVerificacion() {
		return codigoVerificacion;
	}



	public void setCodigoVerificacion(String codigoVerificacion) {
		this.codigoVerificacion = codigoVerificacion;
	}


	@XmlElement
	public String getApellidoPaterno() {
		return apellidoPaterno;
	}



	public void setApellidopaterno(String apellidoPaterno) {
		this.apellidoPaterno = apellidoPaterno;
	}


	@XmlElement
	public String getApellidoMaterno() {
		return apellidoMaterno;
	}



	public void setApellidoMaterno(String apellidoMaterno) {
		this.apellidoMaterno = apellidoMaterno;
	}


	@XmlElement
	public String getNombres() {
		return nombres;
	}



	public void setNombres(String nombres) {
		this.nombres = nombres;
	}


	@XmlElement
	public String getCondicionCasado() {
		return condicionCasado;
	}



	public void setCondicionCasado(String condicionCasado) {
		this.condicionCasado = condicionCasado;
	}


	@XmlElement
	public String getFechaNacimiento() {
		return fechaNacimiento;
	}



	public void setFechaNacimiento(String fechaNacimiento) {
		this.fechaNacimiento = fechaNacimiento;
	}


	@XmlElement
	public String getNombrePadre() {
		return nombrePadre;
	}



	public void setNombrePadre(String nombrePadre) {
		this.nombrePadre = nombrePadre;
	}


	@XmlElement
	public String getNombreMadre() {
		return nombreMadre;
	}



	public void setNombreMadre(String nombreMadre) {
		this.nombreMadre = nombreMadre;
	}


	@XmlElement
	public byte[] getFotoPaciente() {
		return fotoPaciente;
	}



	public void setFotoPaciente(byte[] fotoPaciente) {
		this.fotoPaciente = fotoPaciente;
	}


	@Override
	public String toString() {
		return "PacienteReniecBean [codigoRespuesta=" + codigoRespuesta
				+ ", nroDni=" + nroDni + ", codigoVerificacion="
				+ codigoVerificacion + ", apellidopaterno=" + apellidoPaterno
				+ ", apellidoMaterno=" + apellidoMaterno + ", nombres="
				+ nombres + ", condicionCasado=" + condicionCasado
				+ ", fechaNacimiento=" + fechaNacimiento + ", nombrePadre="
				+ nombrePadre + ", nombreMadre=" + nombreMadre
				+ ", fotoPaciente=" + Arrays.toString(fotoPaciente) + "]";
	}

	
	
	
	

	
	
	
}
