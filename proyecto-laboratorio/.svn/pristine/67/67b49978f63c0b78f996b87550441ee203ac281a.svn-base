package hnch.referencia.core.bean.asistencial.referencia;

import hnch.referencia.base.bean.BaseBean;
import hnch.referencia.core.bean.general.TablaBean;
/*  */
public class ReferenciaDiagnosticoBean extends BaseBean {

	private ReferenciaBean referenciaBean ;
	private TablaBean diagnostico;
	private TablaBean especialidad;
	private TablaBean tipo;
	
	public ReferenciaBean getReferenciaBean() {
		if (referenciaBean == null) {
			referenciaBean = new ReferenciaBean();
		}
		return referenciaBean;
	}
	public void setReferenciaBean(ReferenciaBean referenciaBean) {
		this.referenciaBean = referenciaBean;
	}
	public TablaBean getDiagnostico() {
		if (diagnostico == null) {
			diagnostico = new TablaBean();
		}
		return diagnostico;
	}
	public void setDiagnostico(TablaBean diagnostico) {
		this.diagnostico = diagnostico;
	}
	public TablaBean getEspecialidad() {
		if (especialidad == null) {
			especialidad = new TablaBean();
		}
		return especialidad;
	}
	public void setEspecialidad(TablaBean especialidad) {
		this.especialidad = especialidad;
	}
	public TablaBean getTipo() {
		if (tipo == null) {
			tipo = new TablaBean();
		}
		return tipo;
	}
	public void setTipo(TablaBean tipo) {
		this.tipo = tipo;
	}
	
	
}
